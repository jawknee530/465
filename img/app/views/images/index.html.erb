<p id="notice"><%= notice %></p>

<h1>Listing Images</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Private</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pubImages.each do |image| %>
      <tr>
        <td><%= image.filename %></td>
        <td><%= image.private %></td>
        <td><%= image.user_id %></td>
        <td><%= link_to 'Show', image %></td>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
